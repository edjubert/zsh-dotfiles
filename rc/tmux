#******************************************************************************#

use_tmux() {
  WHOAMI=$(whoami)
  echo "ðŸ›   Loading TMUX session$WHOAMI"
  if [ "$TMUX" = "" ]; then 
    tmux new-session -A -s $WHOAMI; 
  fi
}

